<% if(title=='Services') { %>
<br>

<!-- Carousel -->
<div id="demo" class="carousel slide" data-bs-ride="carousel">

    <!-- Indicators/dots -->
    <div class="carousel-indicators ">
      <button type="button" data-bs-target="#demo" data-bs-slide-to="0" class="active"></button>
      <button type="button" data-bs-target="#demo" data-bs-slide-to="1"></button>
      <button type="button" data-bs-target="#demo" data-bs-slide-to="2"></button>
    </div>
  
    <!-- The slideshow/carousel -->
    <div class="carousel-inner">
      <div class="carousel-item active ">
        <img src="../Assets/images/backend.png" alt="Los Angeles" class="d-block w-100">
        <div class="carousel-caption text-dark">
            <h3>Backend Developer</h3>
            <p></p>
          </div>
      </div>
      <div class="carousel-item">
        <img src="../Assets/images/software.jpg" alt="Chicago" class="d-block w-100">
        <div class="carousel-caption">
            <h3>Full Stack Developer</h3>
            <p></p>
          </div>
      </div>
      </div>
      <div class="carousel-item" style="justify-content: center;" >
        <img src="../Assets/images/mobile.jpg" alt="New York" class="d-block w-100" >
        <div class="carousel-caption text-dark">
            <h3>Mobile Application Developer</h3>
            <p></p>
          </div>
      </div>
      </div>
    </div>
  
    <!-- Left and right controls/icons -->
    <button class="carousel-control-prev" type="button text-dark" data-bs-target="#demo" data-bs-slide="prev">
      <span class="carousel-control-prev-icon text-bg-dark"></span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#demo" data-bs-slide="next">
      <span class="carousel-control-next-icon text-bg-dark"></span>
    </button>
  </div>
  <%}%>
      